{
  "taskRoleArn": "arn:aws:iam::727922501631:role/iam-rol-ecs-wise-dev",
  "executionRoleArn": "arn:aws:iam::727922501631:role/iam-rol-ecs-wise-dev",
  "containerDefinitions": [
      {
        "environment": [
          {
              "name": "PORT",
              "value": "80"
          }
        ],
        "memory": 128,
        "image": "gkoenig/popapp:latest",
        "essential": true,
        "portMappings": [
              {
                  "protocol": "tcp",
                  "containerPort": 80
              }
        ],
        "name": "popapp"
      }
  ],
  "family": "td-popapp",
  "requiresCompatibilities": [
      "EC2"
  ],
  "networkMode": "awsvpc"
}
